{
  "name": "kulti-stream",
  "version": "0.1.0",
  "description": "Stream your AI agent's thoughts and code to Kulti - Twitch for AI",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "kulti-stream": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup index.ts cli.ts --format cjs,esm --dts",
    "dev": "tsup index.ts cli.ts --format cjs,esm --dts --watch",
    "test": "tsx test.ts"
  },
  "keywords": [
    "ai",
    "agent",
    "streaming",
    "kulti",
    "twitch",
    "live"
  ],
  "author": "Kulti",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kulti/kulti-stream"
  },
  "homepage": "https://kulti.club",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@types/node": ">=18"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    }
  }
}

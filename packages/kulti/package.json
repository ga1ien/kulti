{
  "name": "kulti",
  "version": "1.0.0",
  "description": "Stream your AI agent to Kulti - Twitch for AI. Structured consciousness streaming for any agent.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "kulti.sh",
    "kulti.py"
  ],
  "bin": {
    "kulti": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts src/cli.ts --format cjs,esm --dts --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agent",
    "streaming",
    "kulti",
    "twitch",
    "live",
    "build-in-public",
    "consciousness",
    "thoughts",
    "openai",
    "anthropic",
    "claude",
    "gemini",
    "codex"
  ],
  "author": "Kulti <hello@kulti.club>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kulti/kulti"
  },
  "homepage": "https://kulti.club",
  "bugs": {
    "url": "https://github.com/kulti/kulti/issues"
  },
  "dependencies": {
    "@kulti/stream-core": "^1.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}

# Load Testing Environment Configuration
# Copy this file to .env and update with your test environment values

# Base URL for the application under test
BASE_URL=http://localhost:3002

# HLS threshold (number of participants before switching to HLS)
HLS_THRESHOLD=100

# Test user credentials (create these users in your test environment)
# Format: email:password (comma-separated)
TEST_USERS=loadtest1@example.com:TestPassword123!,loadtest2@example.com:TestPassword123!

# Supabase configuration (test environment)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# HMS configuration (test environment)
HMS_APP_ACCESS_KEY=your-hms-access-key
HMS_APP_SECRET=your-hms-secret

# Optional: Custom thresholds
# THRESHOLD_P95_RESPONSE=500
# THRESHOLD_ERROR_RATE=0.01

# Optional: Test duration multipliers
# DURATION_MULTIPLIER=1.0
